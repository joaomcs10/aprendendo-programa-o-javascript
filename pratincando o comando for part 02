//Escreva uma função que apresente como resultado os quadrados dos números inteiros existentes na faixa de valores de 15 a 200.

function quadradoNumerosInteiros(){
    for(let j = 15; j <=200; j++){
        console.log(j * j);
    }
}
quadradoNumerosInteiros();

//Escreva uma função que apresente a soma dos cem primeiros números naturais.

function somaCemPrimeirosNumeros(){
    let somatotal = 0
    for(let j = 1; j <= 100; j++){
        somatotal = somatotal + j
    }
    console.log(somatotal)
}
somaCemPrimeirosNumeros();

//Escreva uma função que apresente todos os valores numéricos menores que 200 que são divisíveis por 4.

function divisiveisPorQuatro(){
    let resultadoFinal = 0
    for(let j = 0; j <= 200;j++){
        if(j % 4 == 0){
           
            console.log(j)
        }
    }
}
divisiveisPorQuatro();

//Escreva uma função que calcule o resultado da soma e da média aritmética dos valores pares situados na faixa numérica de 50 até 70.

function somaEMediaAritmetica(){
    let somaFinal = 0
    let quantidadeDePares = 0 
    for(let j = 50; j <= 70;j++){
        if(j % 2 === 0){
            somaFinal= somaFinal + j;
            quantidadeDePares++;

        }
    }
    const mediaAritimetica = somaFinal / quantidadeDePares;
    console.log(`A soma e ${somaFinal} e a media e ${ mediaAritimetica}`);
}

somaEMediaAritmetica();
